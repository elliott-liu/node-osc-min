{
  "name": "osc-min",
  "version": "1.1.3-pre",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "author": {
    "name": "Russell McClellan",
    "email": "russell.mcclellan@gmail.com",
    "url": "http://www.russellmcc.com"
  },
  "description": "Simple utilities for open sound control in node.js",
  "keywords": [
    "open sound control",
    "OSC",
    "music control",
    "NIME"
  ],
  "repository": {
    "type": "git",
    "url": "http://github.com/russellmcc/node-osc-min.git"
  },
  "dependencies": {
    "binpack": "~0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "@types/node": "^20.12.5",
    "blanket": "*",
    "browserify": "^6.1.0",
    "coffee-script": ">=1.7.1 <2.0.0",
    "coveralls": "*",
    "docket": "0.0.5",
    "mocha": "2.*",
    "mocha-lcov-reporter": "*",
    "tsup": "^8.0.2",
    "typescript": "^5.4.4",
    "uglify-js": "^2.4.15"
  },
  "config": {
    "blanket": {
      "loader": "./node-loaders/coffee-script",
      "pattern": "lib",
      "data-cover-never": "node_modules"
    }
  },
  "directories": {
    "lib": "lib",
    "examples": "examples"
  },
  "license": "Zlib",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "cake test",
    "coverage": "cake coverage",
    "coveralls": "cake coveralls",
    "doc": "cake doc",
    "prepublish": "cake doc; coffee -c lib/osc-utilities.coffee",
    "browserify": "cake browserify",
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "release": "pnpm run build && changeset publish",
    "lint": "tsc",
    "format": "prettier --write src/."
  },
  "_id": "osc-min@0.0.5",
  "dist": {
    "shasum": "ead2d1b1e7ff2382e4dad0f7aaa8aa20832118b1"
  },
  "_from": "osc-min@0.0.5",
  "packageManager": "pnpm@8.15.5+sha256.4b4efa12490e5055d59b9b9fc9438b7d581a6b7af3b5675eb5c5f447cee1a589"
}
